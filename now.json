{
  "version": 2,
  "builds": [
	  { "src": "index.html", "use": "@now/static" },
	  { "src": "date.js", "use": "@now/node" },
	  { "src": "cow.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/date", "dest": "date.js" },
    { "src": "/cow", "dest": "cow.ts" },
    { "src": "/", "dest": "date.js" }
  ]
}

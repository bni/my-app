{
  "version": 2,
  "builds": [
    { "src": "api/headsets.js", "use": "@now/node" },
    { "src": "assets/*", "use": "@now/static" },
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/api/headsets",
      "dest": "/api/headsets.js",
      "headers": {"Access-Control-Allow-Origin": "*"}
    },
    {
      "src": "/api/headsets/(?<id>[^/]*)",
      "dest": "/api/headsets.js?id=$id",
      "headers": {"Access-Control-Allow-Origin": "*"}
    },
    {
      "src": "/detail/(?<id>[^/]*)",
      "dest": "/detail?id=$id"
    }
  ]
}
